#include "split.dtsi"

&five_column_transform {
	col-offset = <5>;
};


&kscan0 {
	col-gpios
		= <&gpio1 4 GPIO_ACTIVE_LOW>  //Col1
		, <&gpio1 5 GPIO_ACTIVE_LOW>  //Col2
		, <&gpio1 6 GPIO_ACTIVE_LOW>  //Col3
		, <&gpio1 7 GPIO_ACTIVE_LOW>  //Col4
		, <&gpio0 8 GPIO_ACTIVE_LOW>  //Col5
		;
};
